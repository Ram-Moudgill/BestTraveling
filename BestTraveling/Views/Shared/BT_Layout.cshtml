@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en" dir="ltr">
<head>
	<!-- required meta information -->
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

	<!-- add font-awesome -->
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
	<link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
	<link href="~/Content/UIContentFiles/style.css" rel="stylesheet" />
	<link href="~/Content/Custom.css" rel="stylesheet" />

	@* Jquery Cdn Link *@
	<script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>

	@* JqueryDataTables *@
	<link href="//cdn.datatables.net/1.10.21/css/jquery.dataTables.min.css" />
	<script src="//cdn.datatables.net/1.10.21/js/jquery.dataTables.min.js"></script>

	<!-- add bootstrap css-->
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">

	<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
	<script src="~/Scripts/toastr.min.js"></script>

	<link href="~/Content/toastr.min.css" rel="stylesheet" />
	@*Sweet alert Cdn link*@

	<script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>

	@*Styles.Render("~/Content/css")
		@Scripts.Render("~/bundles/jquery")
		@Styles.Render("~/Content/toastr")
		@Scripts.Render("~/bundles/toastr")
		@Styles.Render("~/bundles/DataTableStyles")
		@Scripts.Render("~/bundles/DataTableJs")
		@Styles.Render("~/Content1/css")*@

	<title>adminPanel</title>

	<script>
        var SiteUrl = '@BestTraveling.Common_Helpers.CommonProp.SiteUrl' ;
	</script>

</head>
<body>
	<!-- header starting -->
	<header>
		<div class="toggle" id="toggle">
			<i class="fa fa-bars fa-2x"></i>
		</div>
		<nav class="navBg">
			<div class="navBar_logo">
				<a href="@Url.Action("index", "home")">
					<img src="~/Content/images/best.PNG" />
				</a>
			</div>
			<div class="someIcon">
				<a href="#"><i class="fa fa-search"></i></a>
				<a href="#"><i class="fa fa-bell"></i></a>
				<a href="#"><i class="fa fa-power-off"></i></a>
			</div>
		</nav>

		<!-- LeftSideBarDashBoard Starting -->
		<section class="sideBarDashboard">
			@Html.Partial("_Menu")
			<!-- LeftSideBarDashBoard Closed -->
			<div class="container-fluid p-0 container-theme">
				@RenderBody()
				<div class="row m-0 mt-4">
					<div class="col-12">
						<div class="inner">
							<p class="text-center">All rights are reservered &copy; copyright 2020. <a href="#">Best Traveling</a></p>
						</div>
					</div>
				</div>
			</div>
		</section>
	</header>
	<!-- header closing -->
	<script src="~/Content/UIContentFiles/js/index.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js" integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN" crossorigin="anonymous"></script>
	<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js" integrity="sha384-B4gt1jrGC7Jh4AgTPSdUtOBvfO8shuf57BaghqFfPlYxofvL8/KUEfYiJOMMV+rV" crossorigin="anonymous"></script>
</body>
</html>